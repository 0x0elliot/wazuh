<!-- Group BigIP F5 ASM decoders-->
<decoder name="f5_bigip_asm">
  <type>syslog</type>
	<program_name>ASM</program_name>
</decoder>

<!-- General decoder catch all-->
<decoder name="f5_bigip_asm_general">
  <parent>f5_bigip_asm</parent>
  <regex>(.*)</regex>
  <order>message</order>
</decoder>