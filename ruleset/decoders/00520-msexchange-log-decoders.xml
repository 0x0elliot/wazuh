<decoder name="ECPServerLog">
    <prematch>\d*-\d*-\d*T\d*:\d*:\d*.\d*Z,.+,ECP.Request</prematch>
</decoder>

<decoder name="HTTPProxyLog">
    <prematch>\d*-\d*-\d*T\d*:\d*:\d*.\d*Z,[a-z0-9]{8}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{12},</prematch>
</decoder>

<decoder name="OABGenLog">
    <prematch>\d*-\d*-\d*T\d*:\d*:\d*.\d*Z,\.*,OABGenerator</prematch>
</decoder>