{
    "/etc/": {
        "umask": "0o117",
        "format":"plain",
        "type": "file",
        "source": "master",
        "write_mode": "atomic",
        "flags": ["IN_MOVED_TO", "IN_CLOSE_WRITE"],
        "recursive": false
    },

    "/queue/agent-info/": {
        "umask": "0o117",
        "format":"plain",
        "type": "directory",
        "source": "all",
        "write_mode": "atomic",
        "flags": ["IN_CLOSE_WRITE", "IN_ATTRIB", "IN_MOVED_TO"],
        "recursive": false
    },

    "/queue/agent-groups/": {
        "umask": "0o117",
        "format":"plain",
        "type": "directory",
        "source": "master",
        "write_mode": "atomic",
        "flags": ["IN_CLOSE_WRITE", "IN_MOVED_TO"],
        "recursive": false
    },

    "excluded_files": [
        "ar.conf",
        "merged.mg",
        "ossec.conf"
    ]
}
