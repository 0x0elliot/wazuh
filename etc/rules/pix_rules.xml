<!-- Official pix rules for the OSSEC HIDS
  -  Author: Daniel B. Cid
  -  Date: Nov 11, 2005
  -->
     

<!-- For more info:
  -  http://www.cisco.com/en/US/products/sw/secursw/ps2120/products_system_message_guide_chapter09186a00801582b2.html
  -->

<group name="syslog,pix">
  <rule id="2000" level="0">
    <match>^%PIX-</match>
    <description>Pre-match for PIX rules</description>
  </rule>
  
  <rule id="2001" level="9">
    <if_sid>2000</if_sid>
    <match>^%PIX-6-605004: Login denied from</match>
    <description>Attempt to log in the PIX firewall from </description>
    <description>untrusted network.</description>
  </rule>

  <rule id="2002" level="2">
    <if_sid>2000</if_sid>
    <match>^%PIX-5-502103: User priv level changed:</match>
    <description>User changed his privileges in the PIX.</description>
  </rule>

  <rule id="2003" level="2">
    <if_sid>2000</if_sid>
    <match>^%PIX-6-605005: Login permitted from</match>
    <description>Sucessfull login into the PIX firewall.</description>
  </rule>

  <rule id="2004" level="0">
    <if_sid>2000</if_sid>
    <match>^%PIX-6-302010: </match> 
    <description>Connection related message from the PIX firewall.</description>
  </rule>

  <rule id="2005" level="12">
    <if_sid>2000</if_sid>
    <match>^%PIX-6-308001: PIX console enable password incorrect</match>
    <description>Password mismatch while 'enable' on the PIX.</description>
  </rule>

   <rule id="2006" level="8">
    <if_sid>2000</if_sid>
    <match>^%PIX-4-405001: Received ARP response collision from</match>
    <description>ARP collision detected by the PIX.</description>
  </rule> 

   <rule id="2007" level="10">
    <if_sid>2000</if_sid>
    <match>^%PIX-4-401004: Shunned packet: </match>
    <description>Attempt to connect from a shunned IP.</description>
  </rule> 
</group> <!-- SYSLOG,PIX -->


<!-- EOF -->
